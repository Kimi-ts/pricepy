<body>
    <div class="row menu cell well well--smaller">
        <a href="/" name="top" id="top" class="mainLogo"><img ng-src="{{data.header.logo.src}}"></a>
        <ul class="nav nav--horizontal menu" ng-controller="toggleMenuController">
            <li class="desktop-hidden" ng-click="toggleMenu()"><a class="btn btn--noborder menu-item"><i class="fa fa-bars" aria-hidden="true"></i></a></li>
            <li show-when="large" ng-show="isOpen" ng-repeat="item in data.header.menu"><a href="{{item.href}}" class="btn btn--noborder menu-item" ng-class="path == item.path? 'btn--active': ''">{{item.title}}</a></li>
        </ul>
    </div>
    <div ng-view style="min-height:75%"></div>
    <div class="row row--darkbackground cell well grid">
        <div class="textBlock textBlock--smallertext textBlock--leftAligned col col--third">
            <p ng-repeat="item in data.footer.leftColumn.tels" class="textBlock-text--highlited"><a href="{{item.href}}">{{item.title}}</a></p>
            <p ng-repeat="item in data.footer.leftColumn.emails"><a href="{{item.href}}">{{item.title}}</a></p>
            <p ng-repeat="item in data.footer.leftColumn.paragraphs" class="{{item.class}}">{{item.title}}</p>
        </div>
        <div class="textBlock textBlock--smallertext textBlock--leftAligned col col--third">
            <p ng-repeat="item in data.footer.middleColumn">{{item.title}}</p>
        </div>
        <div class="textBlock textBlock--smallertext col col--third">
            <div class="social">
                <a ng-repeat="item in data.footer.socialBar" href="{{item.href}}" target="_blank"><i class="fa {{item.class}}" aria-hidden="true"></i></a>
            </div>
            <nav class="nav nav--white">
               <a ng-repeat="item in data.header.menu" href="{{item.href}}" class="menu-item" ng-class="path == item.path? 'link-active': ''">{{item.title}}</a>
            </nav>
        </div>
        <div class="textBlock col col--fullwidth">
            <hr class="textBlock-horizontalline" />
        </div>
        <div class="textBlock col col--fullwidth">
            <p class="textBlock--smallertext textBlock-text--rightaligned">{{data.footer.copyright.title}}</p>
        </div>
    </div>
    <!— Top100 (Kraken) Widget —>
    <span id="top100_widget"></span>
    <!— END Top100 (Kraken) Widget —>
</body>
</html>