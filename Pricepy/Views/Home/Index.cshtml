<body>
    <div class="d-flex flex-row p-1 px-md-4 mb-1 bg-white border-bottom shadow-sm">
        <a href="/" name="top" id="top" class="my-md-0 mr-md-auto my-2 py-3"><img ng-src="{{data.header.logo.src}}"></a>
        <nav class="my-2 my-md-0 mr-md-3 navbar">
            <a ng-repeat="item in data.header.menu" href="{{item.href}}" class="p-2 nav-item text-warning" ng-class="path == item.path? 'active': ''">{{item.title}}</a>
        </nav>
    </div>
    <div ng-view style="min-height:75%"></div>
    <div class="d-flex flex-column flex-md-row justify-content-between px-3 py-3 py-md-5 px-md-4 mt-5 bg-white border-top">
        <div class="mb-3 mb-md-0">
            <p ng-repeat="item in data.footer.leftColumn.tels" class="mb-0"><a href="{{item.href}}" class="text-warning">{{item.title}}</a></p>
            <p ng-repeat="item in data.footer.leftColumn.emails" class="mb-0"><a href="{{item.href}}" class="text-warning">{{item.title}}</a></p>
            <p ng-repeat="item in data.footer.leftColumn.paragraphs" class="mb-0 {{item.class}}">{{item.title}}</p>
        </div>
        <div class="mb-3 mb-md-0">
            <p ng-repeat="item in data.footer.middleColumn" class="mb-0">{{item.title}}</p>
        </div>
        <div class="mb-3 mb-md-0">
            <a ng-repeat="item in data.footer.socialBar" href="{{item.href}}" target="_blank"><i class="text-warning fa pr-1 {{item.class}}" aria-hidden="true"></i></a>
            <p><a ng-repeat="item in data.header.menu" href="{{item.href}}" class="text-warning pr-1 d-block d-lg-inline" ng-class="path == item.path? 'link-active': ''">{{item.title}}</a></p>
        </div>
        <div class="mb-3 mb-md-0">
            <a class="text-warning" href="#top">Наверх</a>
        </div>
    </div>
    <div class="border-top p-3 text-right">
        <p class="m-0">{{data.footer.copyright.title}}</p>
    </div>
    <!— Top100 (Kraken) Widget —>
    <span id="top100_widget"></span>
    <!— END Top100 (Kraken) Widget —>
