    <script type="text/javascript" src="~/static/dist/ngModules.app.min.js"></script>
    <script type="text/javascript" src="~/static/dist/all.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkNKAC_frei_BLZ0zphi92XjvIkGzudBA" async defer></script>
</body>
</html>